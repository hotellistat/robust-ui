  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "@vueuse/core": "^7.5.5",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.16",
    "d3": "^7.3.0",
    "date-fns": "^2.28.0",
    "gridstack": "^5.0.0",
    "gsap": "^3.9.1",
    "mitt": "^3.0.0",
    "tabbable": "^5.2.1",
    "tailwindcss": "^3.0.17",
    "tippy.js": "^6.3.7",
    "vue": "^3.1.5",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  }
