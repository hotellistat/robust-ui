    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-san": "^1.0.4",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-vue": "^9.0.1",
    "gh-pages": "^4.0.0",
