    "tailwind-config-viewer": "1.7.2",
    "typescript": "5.0.4",
    "vite": "4.3.5",
    "vitepress": "0.22.4",
    "vue": "^3.2.47",
    "vue-eslint-parser": "9.2.1",
    "vue-loader": "17.1.0",
    "vue-tsc": "1.6.4"
  },
  "peerDependencies": {
    "@phosphor-icons/vue": ">= 2.0.0",
    "@vueuse/core": ">= 9.12.0",
    "date-fns": ">= 2.0.0",
