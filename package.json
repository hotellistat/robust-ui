    "tailwind-config-viewer": "1.7.2",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitepress": "0.22.4",
    "vue-eslint-parser": "9.1.0",
    "vue-loader": "17.0.1",
    "vue-tsc": "1.1.7",
    "vue": "^3.2.47"
  },
  "peerDependencies": {
    "@phosphor-icons/vue": ">= 2.0.0",
