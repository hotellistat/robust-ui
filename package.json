{
  "name": "@hotellistat/robust-ui",
  "homepage": "https://hotellistat.github.io/robust-ui",
  "version": "1.6.5",
  "description": "A lightweight Vue 3 component library",
  "repository": "hotellistat/robust-ui",
  "author": "Daniel Schneider <daniel.schneider@hotellistat.de>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/robust-ui.umd.js",
  "browser": "dist/robust-ui.mjs",
  "module": "dist/robust-ui.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/robust-ui.mjs"
    }
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy": "gh-pages -d storybook-static",
    "build": "vite build && vue-tsc --emitDeclarationOnly ",
    "dev": "vue-tsc --emitDeclarationOnly && vite build --watch",
    "deploy-storybook": "storybook-to-ghpages",
    "analyze": "vite build --config analyze.config.js",
    "format": "prettier --write \"src/**/*.{vue,ts,js}\"",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore . --fix",
    "tailwind-config-viewer": "tailwind-config-viewer -o"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@mdx-js/preact": "^2.1.5",
    "@rollup/plugin-typescript": "^9.0.1",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/storybook-deployer": "^2.8.12",
    "@storybook/vue3": "^6.5.12",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/compiler-sfc": "^3.2.40",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "cssnano": "^5.1.13",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-san": "^1.0.5",
    "eslint-plugin-storybook": "^0.6.6",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.6.0",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.17",
    "postcss-import": "^15.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rollup-plugin-visualizer": "^5.8.2",
    "storybook-addon-themes": "^6.1.0",
    "storybook-auto-events": "^0.1.1",
    "storybook-tailwind-dark-mode": "^1.0.15",
    "tailwind-config-viewer": "^1.7.2",
    "typescript": "^4.8.4",
    "vite": "^3.1.7",
    "vitepress": "^0.22.4",
    "vue-eslint-parser": "^9.1.0",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^1.0.5"
  },
  "dependencies": {
    "@dnlsndr/vue-phosphor-icons": "^1.17.0",
    "@popperjs/core": "^2.11.6",
    "@vueuse/core": "^9.3.0",
    "date-fns": "^2.29.3",
    "gsap": "^3.11.3",
    "mitt": "^3.0.0",
    "tailwindcss": "^3.1.8",
    "vee-validate": "^4.7.0",
    "vue": "^3.2.40"
  }
}
