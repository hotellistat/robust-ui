{
  "name": "@hotellistat/robust-ui",
  "homepage": "https://hotellistat.github.io/robust-ui",
  "version": "1.5.0",
  "description": "A lightweight Vue 3 component library",
  "repository": "hotellistat/robust-ui",
  "author": "Daniel Schneider <daniel.schneider@hotellistat.de>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/robust-ui.umd.js",
  "browser": "dist/robust-ui.es.js",
  "module": "dist/robust-ui.es.js",
  "unpkg": "dist/robust-ui.umd.js",
  "jsdelivr": "dist/robust-ui.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/robust-ui.es.js",
      "require": "./dist/robust-ui.umd.js"
    }
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy": "gh-pages -d storybook-static",
    "build": "vite build && vue-tsc --emitDeclarationOnly ",
    "dev": "vue-tsc --emitDeclarationOnly && vite build --watch",
    "deploy-storybook": "storybook-to-ghpages",
    "analyze": "vite build --config analyze.config.js",
    "format": "prettier --write \"src/**/*.{vue,ts,js}\"",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore . --fix",
    "tailwind-config-viewer": "tailwind-config-viewer -o"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@dnlsndr/vue-phosphor-icons": "^1.16.2",
    "@mdx-js/preact": "^2.1.3",
    "@rollup/plugin-typescript": "^8.5.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/storybook-deployer": "^2.8.12",
    "@storybook/vue3": "^6.5.12",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/compiler-sfc": "^3.2.39",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-san": "^1.0.5",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-vue": "^9.5.1",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rollup-plugin-visualizer": "^5.8.1",
    "storybook-addon-themes": "^6.1.0",
    "storybook-auto-events": "^0.1.1",
    "storybook-tailwind-dark-mode": "^1.0.15",
    "tailwind-config-viewer": "^1.7.2",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vitepress": "^0.22.4",
    "vue-eslint-parser": "^9.1.0",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.40.13"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@vueuse/core": "^9.2.0",
    "autoprefixer": "^10.4.12",
    "cssnano": "^5.1.13",
    "d3": "^7.6.1",
    "date-fns": "^2.29.3",
    "gridstack": "^6.0.2",
    "gsap": "^3.11.1",
    "mitt": "^3.0.0",
    "tabbable": "^6.0.0",
    "tailwindcss": "^3.1.8",
    "tippy.js": "^6.3.7",
    "vee-validate": "^4.6.9",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5",
    "vuex": "^4.0.2",
    "yup": "^0.32.11"
  }
}