{
  "name": "@hotellistat/robust-ui",
  "version": "0.2.1",
  "description": "A lightweight Vue 3 component library",
  "repository": "hotellistat/robust-ui",
  "author": "Daniel Schneider <daniel.schneider@hotellistat.de>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/robust-ui.es.js",
  "browser": "dist/robust-ui.es.js",
  "module": "dist/robust-ui.es.js",
  "unpkg": "dist/robust-ui.umd.js",
  "jsdelivr": "dist/robust-ui.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/robust-ui.es.js",
      "require": "./dist/robust-ui.umd.js"
    }
  },
  "scripts": {
    "dev": "start-storybook -p 6006",
    "build": "build-storybook",
    "build:library": "vite build && vue-tsc --emitDeclarationOnly ",
    "analyze": "vite build --config analyze.config.js",
    "format": "prettier --write \"src/**/*.{vue,ts,js}\"",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore . --fix",
    "tailwind-config-viewer": "tailwind-config-viewer -o"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@mdx-js/preact": "^1.6.22",
    "@storybook/addon-actions": "^6.4.16",
    "@storybook/addon-essentials": "^6.4.16",
    "@storybook/addon-links": "^6.4.16",
    "@storybook/vue3": "^6.4.16",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/compiler-sfc": "^3.2.29",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-san": "^1.0.4",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-vue": "^8.4.0",
    "phosphor-vue": "^4.4.0",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "rollup-plugin-visualizer": "^5.5.4",
    "storybook-addon-themes": "^6.1.0",
    "storybook-builder-vite": "^0.1.14",
    "storybook-tailwind-dark-mode": "^1.0.11",
    "tailwind-config-viewer": "^1.6.3",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vitepress": "^0.21.6",
    "vue-eslint-parser": "^8.2.0",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.31.1"
