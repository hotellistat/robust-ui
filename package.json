    "storybook-auto-events": "0.1.1",
    "storybook-tailwind-dark-mode": "1.0.15",
    "tailwind-config-viewer": "1.7.2",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vitepress": "0.22.4",
    "vue-eslint-parser": "9.1.0",
    "vue-loader": "17.0.1",
    "vue-tsc": "1.0.24"
  },
  "dependencies": {
    "@dnlsndr/vue-phosphor-icons": "^3.1.2",
